    "hasha": "^3.0.0",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  }
}
