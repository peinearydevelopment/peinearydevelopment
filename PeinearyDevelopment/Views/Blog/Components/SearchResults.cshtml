@using Contracts;
@using Contracts.Blog;

@model ResultSet<PostSummary>

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<ul>
    @foreach (var post in Model.Results)
    {
        <li>
            <a asp-controller="Blog" asp-action="Post" asp-route-id=@post.Slug>
                @post.Title
            </a>
        </li>
    }
</ul>
